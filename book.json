{
  "plugins": [
    "anchors",
    "edit-link",
    "etoc",
    "exercises",
    "expandable-chapters-small",
    "ga",
    "github",
    "image-captions",
    "quiz",
    "signature",
    "splitter",
    "toggle-chapters",
    "word-count",
    "youtubex"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/jackdougherty/gitbook-on-github/tree/master/",
      "label": "Edit on GitHub"
    },
    "etoc": {
      "mindepth": 2,
      "maxdepth": 4,
      "notoc": false
    },
    "ga": {
      "token": "UA-5488840-29"
    },
    "gitbook-plugin-signature":{
      "autoTimeStamp":{
            "color":"black",
            "timeStampFormat": "YYYY/MM/DD"
          },
          "autoCopyright":{
             "color":"black",
             "owner":"Jack Dougherty and contributors",
             "center":true
          },
          "signature":{
            "key": "customized signature"
          }
    },
    "github": {
      "url": "https://github.com/JackDougherty?tab=repositories"
    }
  }
}
