{
    "plugins": [
        "atoc",
        "edit-link",
        "exercises",
        "expandable-chapters",
        "ga",
        "github",
        "heading-anchors",
        "quizzes",
        "sitemap",
        "splitter",
        "toggle-chapters",
        "word-count",
        "youtube"
    ],
    "pluginsConfig": {
