            "addClass": true,
            "className": "atoc"
        },
        "edit-link": {
                "base": "https://github.com/jackdougherty/gitbook-on-github/tree/dev/",
                "label": "Edit on GitHub"
        },
        "ga": {
            "token": "UA-XXXXX-XX"
        },
        "github": {
            "url": "https://github.com/JackDougherty?tab=repositories"
        },
        {"sitemap": {
            "hostname": "http://YourURLhere.com/"
        }
    }
}
